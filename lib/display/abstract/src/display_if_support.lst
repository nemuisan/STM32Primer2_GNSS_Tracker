 127:lib/display/abstract/src/display_if_support.c **** 	y_ct  = ABS(hhy);
 128:lib/display/abstract/src/display_if_support.c **** 	
 129:lib/display/abstract/src/display_if_support.c **** 	if (wwx) sx = 1; else	 sx =-1;
 130:lib/display/abstract/src/display_if_support.c **** 	if (hhy) sy = 1; else	 sy =-1;
 131:lib/display/abstract/src/display_if_support.c **** 
 132:lib/display/abstract/src/display_if_support.c **** 	x = xs;
 133:lib/display/abstract/src/display_if_support.c **** 	y = ys;
 134:lib/display/abstract/src/display_if_support.c **** 
 135:lib/display/abstract/src/display_if_support.c **** 	if ( x_ct >= y_ct )
 136:lib/display/abstract/src/display_if_support.c **** 	{
 137:lib/display/abstract/src/display_if_support.c **** 	
 138:lib/display/abstract/src/display_if_support.c **** 		e=wwx;
 139:lib/display/abstract/src/display_if_support.c **** 
 140:lib/display/abstract/src/display_if_support.c **** 		for(n=0; n<=x_ct;++n ){
 141:lib/display/abstract/src/display_if_support.c **** 
 142:lib/display/abstract/src/display_if_support.c **** 			PutPixel(x,y,colour);
 143:lib/display/abstract/src/display_if_support.c **** 			x +=sx;
 144:lib/display/abstract/src/display_if_support.c **** 			e += 2*hhy;
 145:lib/display/abstract/src/display_if_support.c **** 
 146:lib/display/abstract/src/display_if_support.c **** 			if (e>=2*wwx){
 147:lib/display/abstract/src/display_if_support.c **** 				e -= 2*wwx;
 148:lib/display/abstract/src/display_if_support.c **** 				y +=sy;
 149:lib/display/abstract/src/display_if_support.c **** 			}
 150:lib/display/abstract/src/display_if_support.c **** 		}
 151:lib/display/abstract/src/display_if_support.c **** 
 152:lib/display/abstract/src/display_if_support.c **** 	}
 153:lib/display/abstract/src/display_if_support.c **** 	else
 154:lib/display/abstract/src/display_if_support.c **** 	{
 155:lib/display/abstract/src/display_if_support.c **** 		
 156:lib/display/abstract/src/display_if_support.c **** 		e=hhy;
 157:lib/display/abstract/src/display_if_support.c **** 
 158:lib/display/abstract/src/display_if_support.c **** 		for(n=0; n<=y_ct;++n ){
 159:lib/display/abstract/src/display_if_support.c **** 		
 160:lib/display/abstract/src/display_if_support.c **** 			PutPixel(x,y,colour);
 161:lib/display/abstract/src/display_if_support.c **** 			y +=sy;
 162:lib/display/abstract/src/display_if_support.c **** 			e += 2*wwx;
 163:lib/display/abstract/src/display_if_support.c **** 
 164:lib/display/abstract/src/display_if_support.c **** 			if (e>=2*hhy){
 165:lib/display/abstract/src/display_if_support.c **** 				e -= 2*hhy;
 166:lib/display/abstract/src/display_if_support.c **** 				x +=sx;
 167:lib/display/abstract/src/display_if_support.c **** 			}
 168:lib/display/abstract/src/display_if_support.c **** 		}
 169:lib/display/abstract/src/display_if_support.c **** 	}
 170:lib/display/abstract/src/display_if_support.c **** 
 171:lib/display/abstract/src/display_if_support.c **** }
